{
  "name": "quexer69/yii2-swiper",
  "description": "Yii2 extension for Swiper slider",
  "keywords": [
    "yii",
    "yii2",
    "extension",
    "widget",
    "swiper",
    "slider"
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.6.0",
    "fxp/composer-asset-plugin": ">=1.0.0",
    "yiisoft/yii2": "^2.0",
    "bower-asset/swiper": "v3.4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.x-dev"
  },
  "autoload": {
    "psr-4": {
      "romkaChev\\yii2\\swiper\\": ""
    }
  },
  "config": {
    "vendor-dir": "vendor",
    "fxp-asset": {
      "git-skip-update": "2 days",
      "vcs-driver-options": {
        "github-no-api": false
      },
      "installer-paths": {
        "bower-asset-library": "vendor/bower"
      },
      "pattern-skip-version": "(-build|-patch)"
    }
}
